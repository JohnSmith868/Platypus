{
    "id": "button-right",
    "components":[
        {
            "type": "DOMElement",
            "element": "div",
            "className": "game-button go-right-button",
            "onmousedown": "start-move",
            "ontouchstart": "start-move",
            "onmouseup": "stop-move",
            "ontouchend": "stop-move",
            "ontouchcancel": "stop-move"
        },{
            "type": "RelayGame",
            "events":{
                "start-move": "button-right:down",
                "stop-move":  "button-right:up"
            }
        }
    ]
}