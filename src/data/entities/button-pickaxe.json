{
	"id": "button-pickaxe",
	"components":[
        {
            "type": "logic-button"
        },{
            "type": "broadcast-events",
		    "events":{
                "mousedown": "button-pickaxe:down",
                "mouseup":   "button-pickaxe:up"
		    }
		},
		{"type": "render-animation",
            "animationMap":{
                "pressed": "pressed",
                "released": "released"
            },
            "spriteSheet": {
                "images": ["buttons"],
                "frames": {
                    "width":  138,
                    "height": 138
                },
                "animations": {
                    "released": 17,
                    "pressed":  23
                }
            },
            "scaleX": 5,
            "scaleY": 5,
            "state": "released",
            "acceptInput": {"click": true, "touch": true}
		}
	],
	"properties":{
		"x": 840,
		"y": 2300,
		"z": 400
	}
}