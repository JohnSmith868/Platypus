{
	"id": "button-right",
	"components":[
        {
		    "type": "logic-button"
		},{
		    "type": "broadcast-events",
            "events": {
                "mousedown": "button-right:down",
                "mouseup":   "button-right:up"
            }
        },
        {"type": "render-animation",
            "spriteSheet": {
                "images": ["buttons"],
                "frames": {
                    "width":  138,
                    "height": 138
                },
                "animations": {
                    "released": 2,
                    "pressed":  8
                }
            },
            "scaleX": 2.9,
            "scaleY": 2.9,
            "state": "released",
            "acceptInput": {"click": true, "touch": true}
		}
	],
	"properties":{
		"x": 1590,
		"y": 1090,
		"width": 400,
		"height": 400
	}
}